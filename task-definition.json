{
  "family": "gym-finance-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::476042145611:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "gym-finance",
      "image": "476042145611.dkr.ecr.eu-west-2.amazonaws.com/gym-finance-cloud:latest",
      "portMappings": [
        { "containerPort": 8000, "protocol": "tcp" }
      ],
      "essential": true,
      "environment": [
        { "name": "ENV", "value": "prod" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/gym-finance",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
